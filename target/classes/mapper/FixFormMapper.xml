<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qcq.dorm.mapper.FixFormMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qcq.dorm.entity.FixForm">
        <id column="id" property="id" />
        <result column="item_id" property="itemId" />
        <result column="submit_time" property="submitTime" />
        <result column="finish_time" property="finishTime" />
        <result column="state" property="state" />
        <result column="satisfaction" property="satisfaction" />
        <result column="comment" property="comment" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, item_id AS itemId, submit_time AS submitTime, finish_time AS finishTime, state, satisfaction, comment
    </sql>

</mapper>
