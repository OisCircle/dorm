<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qcq.dorm.mapper.DormMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qcq.dorm.entity.Dorm">
        <id column="id" property="id"/>
        <result column="building_id" property="buildingId"/>
        <result column="number" property="number"/>
        <result column="clean_score" property="cleanScore"/>
        <result column="electricity_bill" property="electricityBill"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, building_id AS buildingId, number, clean_score AS cleanScore
    </sql>

</mapper>
