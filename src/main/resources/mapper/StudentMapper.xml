<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qcq.dorm.mapper.StudentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qcq.dorm.entity.Student">
        <id column="id" property="id" />
        <result column="password" property="password" />
        <result column="name" property="name" />
        <result column="is_smoke" property="isSmoke" />
        <result column="is_early_sleep" property="isEarlySleep" />
        <result column="habit" property="habit" />
        <result column="bed_id" property="bedId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, password, name, is_smoke AS isSmoke, is_early_sleep AS isEarlySleep, habit, bed_id AS bedId
    </sql>

</mapper>
